import geopandas as gpd

# Read the shapefile
gdf = gpd.read_file('./outline/hubbard_contour.shp')

# Extract the centroid of each line segment
point_gdf = gdf['geometry'].centroid

# Extract X and Y Coordinates
x_coords = point_gdf.geometry.x
y_coords = point_gdf.geometry.y

# Write Coordinates to hubbard_contour.dat
with open('hubbard_contour.dat', 'w') as f:
    for x, y in zip(x_coords, y_coords):
        f.write(f"{x} {y}\n")

